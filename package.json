{
  "name": "iamsteve",
  "version": "8.0.0-beta",
  "private": true,
  "scripts": {
    "dev": "contentlayer dev & next dev",
    "build": "NODE_OPTIONS=--trace-deprecation contentlayer build && next build && node ./scripts/generate-sitemap",
