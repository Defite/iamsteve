    }
  },
  "dependencies": {
    "@netlify/plugin-nextjs": "latest",
    "@next/bundle-analyzer": "^13.4.10",
    "@supabase/auth-helpers-nextjs": "^0.7.3",
    "@supabase/supabase-js": "^2.26.0",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "autoprefixer": "^10.4.14",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "dedent": "^0.7.0",
    "encoding": "^0.1.13",
    "esbuild": "^0.18.15",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "github-slugger": "^2.0.0",
    "globby": "^11.1.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "image-size": "1.0.2",
    "inquirer": "^8.1.1",
    "lint-staged": "^13.2.3",
    "next": "^13.4.10",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.26",
    "preact": "^10.16.0",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-wufoo-embed": "^1.0.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-smartypants": "^2.0.0",
    "sharp": "^0.31.3",
    "swr": "^1.3.0",
    "tailwindcss": "^3.3.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
