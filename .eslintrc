{
  "env": {
    "es6": false,
    "browser": true
  },
  "extends": "airbnb/base",
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "semi": 0,
    "padded-blocks": 0,
    "brace-style": ["error", "stroustrup"],
    "eol-last": 1,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement",
    ],
  }
}
